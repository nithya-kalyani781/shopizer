pipeline {
     agent {label 'jdk11 && mvn '} 
     triggers {
        pollSCM('* * * * *')
     }
         stages {
             stage('clone') { 
                 steps {
                    git url:'https://github.com/nithya-kalyani781/shopizer.git' , branch : 'develop' 
             }
         }
         stage('install') { 
             steps {
                 sh 'mvn package'
             }
         }
   }
}