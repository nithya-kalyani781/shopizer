pipeline {
     agent {label 'jdk11 && mvn '} 
     triggers {
        cron('30 17 * * *')
     }
         stages {
             stage('clone') { 
                 steps {
                    git url:'https://github.com/nithya-kalyani781/shopizer.git' , branch : 'release' 
             }
         }
         stage('install') { 
             steps {
                 sh 'mvn package'
             }
         }
   }
}